#include <stdio.h>
#include <string.h>
int main(){
    char str[1000];
    printf("Enter string\n");
    fgets(str, sizeof(str),stdin);
    int i=0,j,n=strlen(str);

    if(n>0 && str[n-1]=='\n')
    --n;
    j=--n;
    while (i<=j)
    {
        if(str[i++]!=str[j--])
        {
            printf("Not palindrome");
            return 0;
        }   
    }
    
    printf("palindrome");

    return 0;
}
