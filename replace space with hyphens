#include <stdio.h>
#include <string.h>
int main(){
    char str[1000];
    printf("Enter string\n");
    fgets(str,sizeof(str),stdin);
    int n=strlen(str);

    if((str[n-1]='\n')&&(n>0))
    {
        str[n-1]='\0';n--;
    }
    int i =0;
    while(str[i]!='\0')
    {
        if(str[i]==' ' )
        str[i]='-';
        i++;
    }

    printf("\n%s",str);
    return 0;
}
