#include <stdio.h>

int main(){
    int n,odd=0,even=0;
    printf("Enter size of array");
    scanf("%d",&n);
    
    if(n==0)
    {
    printf("size cants be zero");
    return 1;
    }

    int arr[n];
    printf("Enter elements of array");
    for (int i = 0; i < n; i++)
    {
        scanf("%d",&arr[i]);
        if(arr[i]%2==0)
        even++;
        else
        odd++;
    }
    printf("Even=%d, Odd=%d",even,odd);
    
    return 0;
}
