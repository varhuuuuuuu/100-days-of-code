#include <stdio.h>

int main(){
    int n;
    printf("Enter a number");
    scanf("%d",&n);
    unsigned int temp=(unsigned int)n;
    int rev=0;
    while (temp>0)
    {
        int d=temp%10;
        rev=rev*10+d;
        temp=temp/10;
    }
    temp=(unsigned int)n;

    printf("reverse = %d",rev);
    if (temp==rev)
        printf("Number is palindrome");
    else
    printf("Number is not palindrome");
    
    return 0;
}
