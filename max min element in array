#include <stdio.h>

int main(){
    int n;
    printf("Enter size of array");
    scanf("%d",&n);
    int arr[n];
    printf("\nEnter the array\n");
    for (int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    int min=arr[0];
    int max=arr[0];

    for (int i = 1; i <n; i++)
    {
        if(arr[i]<min)
        min=arr[i];
        if(arr[i]>max)
        max=arr[i];
    }
    
    printf("Max=%d",max);
    printf("Min=%d",min);
    return 0;
}
