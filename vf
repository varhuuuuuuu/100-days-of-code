#include <stdio.h>
#include <stdbool.h>

bool prime(int n){
    if (n <= 1) return false;       
    if (n == 2) return true;        
    if (n % 2 == 0) return false;   
    
    for (int i = 3; i * i <= n; i += 2) {
        if (n % i == 0) {
            return false;
        }
    }
    return true;
}


int main(){
    int nn;
    printf("Enter number");
    scanf("%d",&nn);

    for (int i = 1; i <=nn; i++)
    {
        if(prime(i))
        printf("%d ",i);
    }
    

    return 0;
}
