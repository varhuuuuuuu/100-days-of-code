#include <stdio.h>

int main(){
    int n;
    printf("Enter a number\n");
    scanf("%d",&n);
    int k =n;
    printf("Factorial are:\n");
    if(k<0)
     k =-k;

    for(int i =1;i<=k;i++)
    {
        if(n>=0)
        {
        if(k%i==0)
        printf("%d ",i);
        }

        else
        {
         if(k%i==0)
        printf("-%d ",i);   
        }
    }    return 0;  
}
